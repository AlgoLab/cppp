#!/bin/bash -e

parallel --header : echo {gender} {size} \
                         ::: num_species \
                         ::: num_experiments <(seq 0 9) \
                         ::: column_case <(seq 0 5) \
                         ::: num_species \
                         ::: num_species \
                         ::: recombination_rate 0.01 0.05 0.1 0.2 0.4
